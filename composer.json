{
    "name": "mindofmicah/backbone-module-command",
    "description": "Use Laravel's Artisan to quickly create an AMD Backbone Module",
    "authors": [
        {
            "name": "micah",
            "email": "micah@whamdonk.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/filesystem": "4.2.*",
        "mustache/mustache" : "*"
    },
    "require-dev" : {
        "phpspec/phpspec" : "2.*",
        "codeception/codeception" : "2.*"
    },
    "autoload": {
        "psr-4": {
            "MindOfMicah\\Modules\\": "src/",
            "" : "src/"
        }
    },
    "minimum-stability": "stable"
}
